{
  "expo": {
    "name": "modeling-portfolio-mobile",
    "slug": "modeling-portfolio-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "modeling-portfolio",
    "splash": {
      "image": "./src/assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.modelingportfolio.mobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.modelingportfolio.mobile"
    },
    "web": {
      "favicon": "./src/assets/favicon.png",
      "output": "server"
    },
    "plugins": [
      "expo-router",
      "expo-background-task",
      "expo-secure-store",
      "expo-sqlite",
      "expo-web-browser"
    ]
  }
}
