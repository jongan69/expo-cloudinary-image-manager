{
  "name": "modeling-portfolio-mobile",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "dev": "expo start",
    "dev:web": "expo start --web",
    "dev:tunnel": "expo start --tunnel -c",
    "dev:android": "expo run:android --device",
    "dev:ios": "expo run:ios --device",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "prebuild": "bunx expo prebuild",
    "build:all": "bunx eas build --platform all",
    "build:update": "eas update",
    "export:web": "bunx expo export -p web",
    "deploy:web:dev": "bunx expo export -p web && bunx eas-cli@latest deploy",
    "deploy:web:prod": "bunx expo export -p web && bunx eas-cli@latest deploy --prod",
    "db:migrate": "bunx prisma migrate dev",
    "db:generate": "bunx prisma generate --no-engine",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix",
    "format": "prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "format:fix": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "clean": "rm -rf node_modules && rm -rf package-lock.json && bun i",
    "start": "expo start",
    "tunnel": "bunx expo start --tunnel -c",
    "android:dev": "expo run:android --device",
    "ios:dev": "expo run:ios --device",
    "doc-fix": "bunx expo install --check",
    "migrate": "bunx prisma migrate dev",
    "accelerate": "bunx prisma generate --no-engine",
    "swagger:generate": "node scripts/generate-swagger.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@clerk/clerk-expo": "^2.19.2",
    "@expo-google-fonts/outfit": "^0.4.3",
    "@expo/metro-runtime": "~6.1.2",
    "@prisma/extension-accelerate": "^2.0.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-navigation/drawer": "^7.3.9",
    "@reduxjs/toolkit": "^2.10.1",
    "@shopify/flash-list": "2.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "cloudinary": "^2.8.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.5.4",
    "expo": "^54.0.0",
    "expo-auth-session": "~7.0.9",
    "expo-background-task": "~1.0.9",
    "expo-blur": "~15.0.7",
    "expo-constants": "~18.0.10",
    "expo-crypto": "^15.0.7",
    "expo-dev-client": "~6.0.18",
    "expo-document-picker": "~14.0.7",
    "expo-file-system": "~19.0.19",
    "expo-haptics": "~15.0.7",
    "expo-image-picker": "~17.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.9",
    "expo-location": "~19.0.7",
    "expo-navigation-bar": "~5.0.9",
    "expo-router": "~6.0.15",
    "expo-secure-store": "~15.0.7",
    "expo-sharing": "~14.0.7",
    "expo-splash-screen": "~31.0.11",
    "expo-sqlite": "~16.0.9",
    "expo-status-bar": "~3.0.8",
    "expo-updates": "~29.0.12",
    "expo-web-browser": "~15.0.9",
    "idb": "^8.0.3",
    "lottie-react-native": "~7.3.1",
    "lucide-react-native": "^0.542.0",
    "nativewind": "^4.2.1",
    "prettier": "^3.6.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-actions-sheet": "^0.9.8",
    "react-native-calendars": "^1.1313.0",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-modal": "^14.0.0-rc.1",
    "react-native-reanimated": "~4.1.1",
    "react-native-ruler-picker": "^0.2.2",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-toast-message": "^2.3.3",
    "react-native-web": "^0.21.0",
    "react-native-wheel-picker-expo": "^0.5.4",
    "react-redux": "^9.2.0",
    "redux": "^5.0.1",
    "redux-devtools-expo-dev-plugin": "^0.2.1",
    "semver": "^7.7.3",
    "sql.js": "^1.13.0",
    "swagger-jsdoc": "^6.2.8",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@expo/ngrok": "^4.1.3",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.1.10",
    "@types/sql.js": "^1.4.9",
    "eslint": "^8.57.1",
    "eslint-config-expo": "~10.0.0",
    "jest": "^29.7.0",
    "jest-expo": "~54.0.13",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "sqlite3": "^5.1.7",
    "tailwindcss": "^3.4.18",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.2"
  },
  "private": true,
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "@prisma/extension-accelerate",
          "eslint-config-prettier",
          "eslint-plugin-prettier",
          "idb",
          "prettier",
          "react-native-wheel-picker-expo",
          "redux-devtools-expo-dev-plugin",
          "sql.js",
          "react-native-chart-kit"
        ],
        "listUnknownPackages": false
      }
    }
  }
}